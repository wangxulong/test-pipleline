pipeline {
    agent any
    stages{
        stage("开始"){
            steps{
                script{
                    withCredentials([usernamePassword(
                            credentialsId: 'ded8a991-4157-4e65-ba75-fc43af77a9ba',
                            passwordVariable: 'password',
                            usernameVariable: 'username')]) {
                        echo "${username},${password}"
                    }
                }

            }


        }
    }
}